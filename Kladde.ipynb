   "source": [
    "$$\n",
    "I = I_0 \\cdot e^{-\\epsilon C\\ln(10) \\cdot d}\n",
    "$$  "
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 10,
   "id": "60b74856",
   "metadata": {},
   "outputs": [
    {
     "data": {
      "text/latex": [
       "$\\displaystyle \\varepsilon = \\frac{\\log{\\left(\\frac{I_{0}}{I} \\right)}}{C d \\log{\\left(10 \\right)}}$"
      ],
      "text/plain": [
       "Eq(varepsilon, log(I0/I)/(C*d*log(10)))"
      ]
     },
     "execution_count": 10,
     "metadata": {},
     "output_type": "execute_result"
    }
   ],
   "source": [
    "i, I0, epsilon, C, d = symbols(\"I I0 varepsilon C d\")\n",
    "lign = Eq(i, I0 * exp(-epsilon * C * ln(10) * d))\n",
    "\n",
    "Eq(epsilon, solve(lign, epsilon)[0])"
   ]
  }
